{
    "name": "<%= _.slugify(appname) %>",
    "version": "0.0.0",
    "main": "index.html",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components"
    ],
    "dependencies": {
        <% if(uirouter) { print('"angular-ui-router": ">=0.2",'); } else { print('"angular-route": "~1",'); } %>
        "jquery": "~2",
        "lodash": "~2",
        "bootstrap": "~3",
        "angular": "~1",
        "angular-route": "~1",
        "angular-animate": "~1",
        "angular-resource": "~1",
        "angular-messages": "~1",
        "angular-cookies": "~1",
        "angular-mocks": "~1",
        "angular-sanitize": "~1",
        "angular-ui-utils": "~0.1",
        "angular-bootstrap": ">=0.12",
        "moment": "~2",
        "font-awesome": "~4",

    },
    "overrides": {
        "angular-bootstrap": {
            "main": "ui-bootstrap-tpls.js"
        },
        "bootstrap": {
            "main": [
                "js/affix.js"
            ]
        },
        "angular-toggle-switch": {
            "main": [
                "angular-toggle-switch.min.js",
                "angular-toggle-switch.css"
            ]
        }
    }
}
