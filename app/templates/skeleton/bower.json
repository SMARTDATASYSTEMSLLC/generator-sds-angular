{
    "name": "<%= _.slugify(appname) %>",
    "version": "0.0.0",
    "main": "app/index.html",
    "appPath": "app",
    "distPath": "dist",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components"
    ],
    "dependencies": {
        <% if(uirouter) {
          print('"angular-router": "angular/router",');
        } else {
          print('"angular-route": "~1",');
        } %>
        "lodash": "~3",
        "bootstrap": "~3",
        "bootswatch": "~3",
        "angular": "~1.4",
        "angular-animate": "~1.4",
        "angular-resource": "~1.4",
        "angular-messages": "~1.4",
        "angular-cookies": "~1.4",
        "angular-mocks": "~1.4",
        "angular-sanitize": "~1.4",
        "angular-ui-utils": "~0.1",
        "angular-bootstrap": "~0.11",
        "moment": "~2",
        "less.js": "~2.0",
        "font-awesome": "~4.2",
        "blockui": "*"
    },
    "overrides": {
        "angular-bootstrap": {
            "dependencies": {
                "angular": "~1"
            },
            "main": "ui-bootstrap-tpls.js"
        },
        "bootstrap": {
            "main": [
                "js/affix.js"
            ]
        },
        "angular-toggle-switch": {
            "main": [
                "angular-toggle-switch.min.js",
                "angular-toggle-switch.css"
            ]
        }
    },
    "resolutions": {
        "angular": "~1.4",
        "jquery": ">2.1.0"
    }
}
