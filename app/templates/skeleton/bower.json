{
    "name": "<%= _.slugify(appname) %>",
    "version": "1.0.0",
    "main": "<%= appdir %>/index.html",
    "appPath": "<%= appdir %>",
    "distPath": "<%= distdir %>",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components"
    ],
    "dependencies": {
        <% if(uirouter) {
          print('"angular-router": "angular/router",');
        } else {
          print('"angular-route": "~1",');
        } %>
        <% if (hasAuth){
          print('"angular-jwt": "~0.0.7",');
        } %>
        "lodash": "~3",
        "bootstrap": "~3",
        <% if (bootswatch){
            print('"bootswatch": "~3",');
        } %>
        <% if (adminlte){
            print('"admin-lte": "~2",');
        } %>
        "angular": "~1.4",
        "angular-animate": "~1.4",
        "angular-resource": "~1.4",
        "angular-messages": "~1.4",
        "angular-cookies": "~1.4",
        "angular-mocks": "~1.4",
        "angular-sanitize": "~1.4",
        "angular-ui-utils": "~0.1",
        "angular-bootstrap": "~0.11",
        "moment": "~2",
        "font-awesome": "~4",
        "blockui": "*"
    },
    "overrides": {
        "angular-bootstrap": {
            "dependencies": {
                "angular": "~1"
            },
            "main": "ui-bootstrap-tpls.js"
        },
        "bootstrap": {
            "main": [
                "js/affix.js",
                "js/tooltip.js"
            ]
        },
        "angular-toggle-switch": {
            "main": [
                "angular-toggle-switch.min.js",
                "angular-toggle-switch.css"
            ]
        },
        "admin-lte": {
            "main": [
                "dist/js/app.js",
                "build/less/AdminLTE.less"
            ]
        }
    },
    "resolutions": {
        "angular": "~1.4",
        "jquery": ">2.1.0"
    }
}
