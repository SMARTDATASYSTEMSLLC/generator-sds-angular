{
    "name": "<%= _.slugify(appname) %>",
    "version": "0.0.0",
    "main": "index.html",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components"
    ],
    "dependencies": {
        <% if(uirouter) { print('"angular-ui-router": "~0.2.13",'); } else { print('"angular-route": "~1",'); } %>
        "lodash": "~3",
        "bootstrap": "~3",
        "angular": "~1.3",
        "angular-route": "~1.3",
        "angular-animate": "~1.3",
        "angular-resource": "~1.3",
        "angular-messages": "~1.3",
        "angular-cookies": "~1.3",
        "angular-mocks": "~1.3",
        "angular-sanitize": "~1.3.3",
        "angular-ui-utils": "~0.1",
        "angular-bootstrap": "~0.11",
        "angular-local-storage": "~0.1.5",
        "moment": "~2",
        "less.js": "~2.0",
        "font-awesome": "~4.2",
        "blockui": "*"
    },
    "overrides": {
        "angular-bootstrap": {
            "main": "ui-bootstrap-tpls.js"
        },
        "bootstrap": {
            "main": [
                "js/affix.js"
            ]
        },
        "angular-toggle-switch": {
            "main": [
                "angular-toggle-switch.min.js",
                "angular-toggle-switch.css"
            ]
        }
    },
    "resolutions": {
        "angular": "~1.3",
        "jquery": ">2.1.0"
    }
}
